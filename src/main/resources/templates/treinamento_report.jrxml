	<parameter name="ID_FUNCIONARIO" class="java.lang.Long">
		<defaultValueExpression><![CDATA[1L]]></defaultValueExpression>
	</parameter>
	<query language="sql"><![CDATA[SELECT 
    -- TODOS OS CAMPOS DE TREINAMENTO
    t.id AS treinamento_id,
    t.data_agendamento AS treinamento_data_agendamento,
    t.data_concluido AS treinamento_data_concluido,
    t.data_vencimento AS treinamento_data_vencimento,
    t.instrutor AS treinamento_instrutor,
    t.status AS treinamento_status,
    t.curso_id AS treinamento_curso_id,
    t.funcionario_id AS treinamento_funcionario_id,
    
    -- TODOS OS CAMPOS DE FUNCIONÁRIO
    f.id AS funcionario_id,
    f.cargo AS funcionario_cargo,
    f.cpf AS funcionario_cpf,
    f.data_admissao AS funcionario_data_admissao,
    f.data_nascimento AS funcionario_data_nascimento,
    f.departamento AS funcionario_departamento,
    f.email AS funcionario_email,
    f.matricula AS funcionario_matricula,
    f.nome AS funcionario_nome,
    f.rg AS funcionario_rg,
    f.situacao AS funcionario_situacao,
    f.telefone AS funcionario_telefone,
    f.tipo_contrato AS funcionario_tipo_contrato,
    
    -- TODOS OS CAMPOS DE CURSO
    c.id AS curso_id,
    c.carga_horaria AS curso_carga_horaria,
    c.descricao AS curso_descricao,
    c.nome AS curso_nome,
    c.origem_curso AS curso_origem_curso,
    c.tipo_obrigatoriedade AS curso_tipo_obrigatoriedade,
    c.validade_meses AS curso_validade_meses
    
FROM projeto_rest_spring_boot.treinamento t
INNER JOIN projeto_rest_spring_boot.funcionario f 
    ON t.funcionario_id = f.id
INNER JOIN projeto_rest_spring_boot.curso c 
    ON t.curso_id = c.id
WHERE f.id = $P{ID_FUNCIONARIO}
ORDER BY t.data_agendamento DESC]]></query>
